{
  "name": "yt-astro-ui",
  "version": "0.1.1",
  "description": "A component library built for Astro and Svelte",
  "homepage": "https://astro-ui.dev.yatoday.es",
  "bugs": "https://github.com/qwert2001qwert-alex/yt-astro-ui/issues",
  "license": "MIT",
  "author": "Alex <qwert2001qwert@gmail.com> (https://yatoday.es)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qwert2001qwert-alex/yt-astro-ui.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./src/types.d.ts",
        "default": "./index.js"
      }
    },
    "./*": "./src/*",
    "./utils": "./src/utils/index.ts",
    "./types": "./src/types.d.ts",
    "./integration": "./src/vendor-config/index.ts"
  },
  "main": "./index.js",
  "types": "./src/types.d.ts",
  "keywords": [
    "astro-component"
  ],
  "scripts": {
    "dev": "cd ./demo && astro dev",
    "build": "cd ./demo && astro build",
    "preview": "cd ./demo && astro preview",
    "astro": "astro",
    "pub": "npm version patch && npm publish --access public",
    "link": "cd ./demo && npm link yt-astro-ui",
    "check": "npm run check:astro && npm run check:eslint && npm run check:prettier",
    "check:astro": "astro check",
    "check:eslint": "eslint .",
    "check:prettier": "prettier --check .",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier -w ."
  },
  "dependencies": {
    "@astrolib/seo": "^1.0.0-beta.8",
    "astro-icon": "^1.1.5",
    "limax": "4.1.0",
    "lodash.merge": "^4.6.2",
    "photoswipe": "^5.4.4",
    "swiper": "^11.2.4",
    "unpic": "^3.22.0"
  },
  "devDependencies": {
    "@astrojs/svelte": "^7.0.4",
    "@astrolib/seo": "^1.0.0-beta.8",
    "@types/html-escaper": "^3.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "astro": "^5.0.0",
    "bits-ui": "^1.1.0",
    "clsx": "^2.1.1",
    "svelte": "^5.20.1",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "^4.0.6"
  },
  "peerDependencies": {
    "@astrojs/svelte": "^7.0.4",
    "@astrolib/seo": "^1.0.0-beta.8",
    "astro": "^5.0.0",
    "astro-icon": "^1.1.5",
    "bits-ui": "^1.1.0",
    "clsx": "^2.1.1",
    "svelte": "^5.20.1",
    "tailwind-merge": "^3.0.0",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^4.0.0"
  }
}
